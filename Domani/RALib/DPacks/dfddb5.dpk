  fDataSetEditor in 'fDataSetEditor.pas' {DatasetEditor},
  fDBGridColumnsEditor in 'fDBGridColumnsEditor.pas' {DBGridColumnsEditor},
  fAddFields in 'fAddFields.pas' {AddFields};

