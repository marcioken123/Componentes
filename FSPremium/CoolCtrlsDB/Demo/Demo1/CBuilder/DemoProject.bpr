[HistoryLists\hlIncludePath]
Count=2
Item0=$(BCB)\include;$(BCB)\include\vcl;c:\cdbu_src\reg\CBuilder3\
Item1=$(BCB)\include;$(BCB)\include\vcl;c:\cdbu_src\reg\CBuilder3[HistoryLists\hlLibraryPath]

[HistoryLists\hlLibraryPath]
Count=1
Item0=..\..\..\cdbu_src\reg\cbuilder3;c:\cdbu_src\reg\CBuilder3\;$(BCB)\lib\obj;$(BCB)\lib

[HistoryLists\hlDebugSourcePath]
