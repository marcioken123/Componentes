# ---------------------------------------------------------------------------
!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.03
# ---------------------------------------------------------------------------
PROJECT = DemoProject.exe
OBJFILES = DemoProject.obj UnitMain.obj
RESFILES = DemoProject.res
DEFFILE =
RESDEPEN = $(RESFILES) UnitMain.dfm
LIBFILES =
LIBRARIES = CdbuB3V104.lib VCLX35.lib VCLDB35.lib VCL35.lib
SPARELIBS = VCL35.lib VCLDB35.lib VCLX35.lib CdbuB3V104.lib
PACKAGES = VCLX35.bpi VCL35.bpi VCLDB35.bpi VCLDBX35.bpi bcbsmp35.bpi dclocx35.bpi \
  QRPT35.bpi TEEUI35.bpi VCLSMP35.bpi TEEDB35.bpi TEE35.bpi ibsmp35.bpi DSS35.bpi \
  NMFAST35.bpi INETDB35.bpi INET35.bpi VCLMID35.bpi dxcomnC3.bpi dxInsC3.bpi \
  DXDBEDC3.bpi DXEDTRC3.bpi EQTLC3.bpi EQGridC3.bpi
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
# ---------------------------------------------------------------------------
CFLAG1 = -Od -Hc -w -Ve -r- -k -y -v -vi- -c -b- -w-par -w-inl -Vx -tW
CFLAG2 = -D_RTLDLL -I$(BCB)\include;$(BCB)\include\vcl;c:\cdbu_src\reg\CBuilder3\ \
  -H=$(BCB)\lib\vcl35.csm
CFLAG3 = -Tkh30000
PFLAGS = -D_RTLDLL \
  -U..\..\..\cdbu_src\reg\cbuilder3;c:\cdbu_src\reg\CBuilder3\;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I$(BCB)\include;$(BCB)\include\vcl;c:\cdbu_src\reg\CBuilder3\ -$Y -$W -$O- -v \
  -JPHN -M
RFLAGS = -D_RTLDLL -i$(BCB)\include;$(BCB)\include\vcl;c:\cdbu_src\reg\CBuilder3\ = \
  /i$(BCB)\include /i$(BCB)\include\vcl /ic:\cdbu_src\reg\CBuilder3\ /d_RTLDLL \
  /dUSEPACKAGES /mx /w2 /zd
AFLAGS = /i$(BCB)\include /i$(BCB)\include\vcl /ic:\cdbu_src\reg\CBuilder3\ /d_RTLDLL \
  /mx /w2 /zd
LFLAGS = -L..\..\..\cdbu_src\reg\cbuilder3;c:\cdbu_src\reg\CBuilder3\;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -aa -Tpe -x -Gn -v
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0w32.obj sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mti.lib
# ---------------------------------------------------------------------------
!ifdef IDEOPTIONS

[Version Info]
IncludeVerInfo=0
AutoIncBuild=0
MajorVer=1
MinorVer=0
Release=0
Build=0
